package ADS.src.BinarySearchTree;

public class Node
{
   private Node leftchild;
   private Node rightchild;
   private int key;
   private int hit;

   public Node(int key) {

      this.key = key;
      this.hit = 0;
   }

   public String toString() {
      return "Node leftchild: " + leftchild + " Rightchild: " + rightchild
            + " Key: " + key + ", Hit: " + hit;
   }

   public int getHit() {
      return hit;
   }
   
}